{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "template-3": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist/template"
            },
            "index": "src/index.html",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/icons/fontawesome/css/fontawesome.min.css",
              "src/assets/icons/fontawesome/css/all.min.css",
              "src/assets/icons/feather/css/iconfont.css",
              "src/assets/icons/tabler-icons/tabler-icons.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/scss/owl.carousel.scss",
              "node_modules/ngx-owl-carousel-o/lib/styles/scss/owl.theme.default.scss",
              "node_modules/slick-carousel/slick/slick.scss",
              "./node_modules/ngx-lightbox/lightbox.css",
              "node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
              "node_modules/slick-carousel/slick/slick-theme.scss",
              "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/aos/dist/aos.css",
              "node_modules/intl-tel-input/build/css/intlTelInput.css",
              "src/styles.css",
              "src/custom-theme.scss"
            ],
            "scripts": [
              "node_modules/intl-tel-input/build/js/intlTelInput.min.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/slick-carousel/slick/slick.min.js",
              "node_modules/aos/dist/aos.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.js"
            ],
            "browser": "src/main.ts"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "3mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "template-3:build:production"
            },
            "development": {
              "buildTarget": "template-3:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "template-3:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "6012202b-37b7-4228-b88e-60cee27211a6"
  }

}
